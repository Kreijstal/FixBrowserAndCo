use "classes";

import "util/common";

function is_same_domain(src: String, dest: String): Boolean
{
	if (domain_ends_with(dest, "i.guim.co.uk")) return true;
	if (domain_ends_with(dest, "assets.guim.co.uk")) return true;
	return false;
}

