use "classes";

import "util/common";

function adjust_document(request: Request, document: Element)
{
	rewrite_links(document, rewrite_link#3, null);
}

function @rewrite_link(elem: Element, value: String, data): String
{
	var url = URL::create(value);
	var domain = url.get_host();
	if (domain == "davityle.github.io") {
		url.set_host("tyler-davis.com");
		return url.to_string();
	}
	return value;
}
