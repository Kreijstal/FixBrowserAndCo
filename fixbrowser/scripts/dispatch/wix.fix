use "classes";

import "util/common";
import "util/string";

function adjust_document(request: Request, document: Element)
{
	var iframe = document.query("iframe[id^='TPAMultiSection_']");
	if (iframe) {
		var src = iframe.get_attr("src");
		if (src.starts_with("https://social-blog.wix.com/")) {
			request.set_same_domain_url(src);
		}
	}
}
