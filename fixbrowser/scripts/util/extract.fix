use "classes";

import "util/string";

function extract_string(s: String, prefix: String, suffix: String): String
{
	var start = s.search_string(prefix);
	if (start == -1) return null;
	start += prefix.length;
	
	var end = s.search_string(suffix, start);
	if (end == -1) return null;
	return s.substring(start, end);
}
